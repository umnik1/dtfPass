{
    "manifest_version": 2,
    "name": "DTFPass DEV",
    "description": "Extention for DTF",
    "version": "0.0.1",
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions":[
        "https://dtf.ru/*",
        "*://*.osnova.io/*",
        "unlimitedStorage",
        "storage"],
    "content_scripts": [{
        "matches": [
         "*://*.dtf.ru/*",
         "*://*.osnova.io/*"
        ],
        "js": [
         "js/content.js"
        ],
        "css": ["css/pass.css"],
        "run_at": "document_end"
        }],
        "web_accessible_resources": [
            "js/*"
        ]
}